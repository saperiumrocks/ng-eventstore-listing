{
  "name": "ng-eventstore-listing-showcase",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve",
    "test": "ng test",
    "lint": "cd lib && ng lint && cd ..",
    "e2e": "ng e2e",
    "test:lib": "ng test --watch=false --sourceMap=false --browsers=ChromeHeadless",
    "build": "npm run build:lib && npm run build:showcase",
    "old:build:lib": "npm run copy:readme && ng build ng-eventstore-listing --prod",
    "build:lib": "ng-packagr -p lib/package.json",
    "publish:lib": "cd lib/dist && npm publish && cd .. && cd ..",
    "build:showcase": "ng build ng-eventstore-listing-showcase --base-href='https://saperiumrocks.github.io/ng-eventstore-listing/'",
    "publish:demo": "npx angular-cli-ghpages --dir=./dist/ng-eventstore-listing-showcase",
    "test:showcase": "ng test ng-eventstore-listing-showcase --watch=false --sourceMap=false --browsers=ChromeHeadless",
    "copy:readme": "copyfiles ./README.md ./dist/ng-eventstore-listing"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.0",
    "@angular/common": "5.2.0",
    "@angular/compiler": "5.2.0",
    "@angular/core": "5.2.0",
    "@angular/forms": "5.2.0",
    "@angular/http": "5.2.0",
    "@angular/platform-browser": "5.2.0",
    "@angular/platform-browser-dynamic": "5.2.0",
    "@angular/router": "5.2.0",
    "bootstrap": "^4.5.0",
    "copyfiles": "^2.3.0",
    "core-js": "^2.4.1",
    "jquery": "^3.5.1",
    "ng-eventstore-listing": "file:lib/dist",
    "ngx-bootstrap": "^5.6.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular-devkit/core": "0.0.29",
    "@angular/cli": "1.6.0",
    "@angular/compiler-cli": "5.2.0",
    "@angular/language-service": "5.2.0",
    "@types/node": "~6.0.60",
    "@types/jasmine": "2.5.45",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^2.4.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "^2.4.2"
  }
}
