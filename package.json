{
  "name": "ng-eventstore-listing",
  "version": "0.1.1-alpha",
  "description": "A listing library utilizing an eventstore implementation",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test:lib": "ng test ng-eventstore-listing --watch=false --sourceMap=false --browsers=ChromeHeadless",
    "build": "npm run build:lib && npm run build:showcase",
    "build:lib": "npm run copy:readme && ng build ng-eventstore-listing --prod",
    "publish:lib": "cd dist/ng-eventstore-listing && npm publish && cd .. && cd ..",
    "build:showcase": "ng build ng-eventstore-listing-showcase --base-href='https://saperiumrocks.github.io/ng-eventstore-listing/'",
    "publish:demo": "npx angular-cli-ghpages --dir=./dist/ng-eventstore-listing-showcase",
    "test:showcase": "ng test ng-eventstore-listing-showcase --watch=false --sourceMap=false --browsers=ChromeHeadless",
    "lint": "ng lint ng-eventstore-listing && ng lint ng-eventstore-listing-showcase",
    "e2e": "ng e2e",
    "copy:readme": "copyfiles ./README.md ./dist/ng-eventstore-listing",
    "format:check": "prettier --list-different 'projects/**/*.ts'",
    "format:write": "prettier --write 'projects/**/*.ts'",
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
    "adjust-version": "npm-run-all adjust-version:*",
    "adjust-version:lib": "rjp projects/ng-simple-alert/package.json version $VERSION",
    "adjust-version:main": "rjp package.json version $VERSION"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "projects/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~9.1.7",
    "@angular/common": "~9.1.7",
    "@angular/compiler": "~9.1.7",
    "@angular/core": "~9.1.7",
    "@angular/forms": "~9.1.7",
    "@angular/platform-browser": "~9.1.7",
    "@angular/platform-browser-dynamic": "~9.1.7",
    "@angular/router": "~9.1.7",
    "bootstrap": "^4.5.0",
    "copyfiles": "^2.3.0",
    "fs": "0.0.1-security",
    "immutable": "^4.0.0-rc.12",
    "lodash-es": "^4.17.15",
    "ngx-bootstrap": "^5.6.1",
    "path": "^0.12.7",
    "rxjs": "~6.5.4",
    "rxjs-compat": "^6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular-devkit/build-ng-packagr": "~0.901.7",
    "@angular/cli": "~9.1.6",
    "@angular/compiler-cli": "~9.1.7",
    "@semantic-release/exec": "^5.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "^10.2.10",
    "ng-packagr": "^9.0.0",
    "prettier": "^2.0.5",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
